    "cors": "^2.8.5",
    "dotenv": "^7.0.0",
    "express": "^4.16.4",
    "helmet": "^3.16.0",
    "jsonwebtoken": "^9.0.0",
    "knex": "^0.19.5",
    "npm-run-all": "^4.1.5",
    "pg": "^7.9.0",
    "sqlite3": "^4.0.6"
